{
  "name": "connexik-extension",
  "displayName": "Connexik Extension",
  "version": "0.0.1",
  "description": "Automate your LinkedIn networking with smart tools to accept connection requests and grow your professional reach effortlessly.",
  "author": "Deepam Kapur <deepamkapur@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "pnpm exec eslint .",
    "lint:fix": "pnpm exec eslint . --fix"
  },
  "dependencies": {
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@parcel/core": "^2.13.3",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "*://www.linkedin.com/*",
      "*://linkedin.com/*"
    ],
    "permissions": [
      "identity",
      "tabs",
      "storage",
      "alarms",
      "scripting"
    ]
  }
}
